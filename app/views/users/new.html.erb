<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :first_name %>
  <%= f.password_field :first_name %>
  <%= f.label :last_name %>
  <%= f.password_field :last_name %>
  <%= f.submit "Create Account"%>
<% end %>

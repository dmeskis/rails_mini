<h1 class='text-center'><%= @form.student.last_name %>, <%= @form.student.first_name %></h1>
<h3 class='text-center'><%= @date %></h3>

<%= @form.mood %>
<%= @form.worked_hard_at %>
<%= @form.done_better_at %>
<%= @form.favorite_part %>

<% if @form.confirmed == 0 %>
<%= button_to "Confirm", teacher_form_confirm_path(@form, confirmed: 1), :method => :put %>
<% end %>
<%= button_to "Delete", teacher_form_path(@form), :method => :delete %>
